{
  "name": "base",
  "version": "0.0.0",
  "scripts": {
    "all": "npm run test && npm run lint && npm run build",
    "build": "ng build --configuration production",
    "engines": {
      "node": "16.x",
      "npm": "7.x"
    },
    "heroku-prebuild": "npm install --force",
    "doc": "rimraf reports/Documentation && compodoc --theme stripe -p tsconfig.compodoc.json -d reports/Documentation -n base",
    "e2e": "ng e2e",
    "lint": "ng lint base",
    "mock:server": "json-server --watch db.json",
    "ng": "ng",
    "prebuild": "npm i",
    "start-ng": "ng serve base",
    "start" : "node server.js",
    "start:proxy:mock:server": "concurrently  -k -s first \"npm run mock:server\" \"npm run start\"",
    "test": "ng test base --no-watch --code-coverage && npm run lint",
    "test-nowatch": "npm run lint && ng test base --watch=false --browsers ChromeHeadless"
  },
  "private": true,
  "dependencies": {

    "@angular/animations": "~12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/flex-layout": "~12.0.0-beta.35",
    "@angular/forms": "~12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@compodoc/compodoc": "^1.1.15",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "classlist.js": "^1.1.20150312",
    "concurrently": "^6.3.0",
    "core-js": "^3.10.1",
    "express": "^4.17.3",
    "isomorphic-fetch": "^3.0.0",
    "json-server": "^0.16.0",
    "ng-http-loader": "^9.1.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "rxjs": "^6.6.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.16",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.12",
    "caniuse-lite": "1.0.30001124",
    "codelyzer": "^6.0.0",
    "electron-to-chromium": "1.3.413",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.5.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^2.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-sonarqube-unit-reporter": "^0.0.23",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
